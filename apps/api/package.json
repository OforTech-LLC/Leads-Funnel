    "@aws-sdk/lib-dynamodb": "^3.500.0",
    "@aws-sdk/s3-request-presigner": "^3.500.0",
    "@kanjona/shared": "*",
    "jose": "^5.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/node": "^22.0.0",
    "@types/uuid": "^9.0.0",
    "@vitest/coverage-v8": "^1.3.1",
